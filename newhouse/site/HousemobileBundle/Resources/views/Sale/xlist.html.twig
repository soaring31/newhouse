    {% extends app.request.get('ajax') ? 'HousemobileBundle:Clist:school_list.html.twig' : 'HousemobileBundle::list_layout.html.twig' %}

{% block title %}
    【{{cur_area_data|default('')}}学校附近二手房出售|{{cur_area_data|default('')}}学校小区价格】
{% endblock title %}

{% block key_des %}
<meta name="keywords" content="{{cur_area_data|default('')}}学校附近二手房，{{cur_area_data|default('')}}学校小区" />
<meta name="description" content="{{core_global.mconfig({name: 'hostname', ename: 'mwebset'})}}{{cur_area_data|default('')}}二手房网为您提供快速全面的{{cur_area_data|default('')}}学校附近二手房出售信息及{{cur_area_data|default('')}}学校小区信息。大量优质教育房源实时更新，为您提供舒适教育房购房体验。" />
{% endblock key_des %}

{% block header_title %}
{{core_mobile_global.title({title: '学区'})}}
{% endblock header_title %}

{# 头部右边追加 #}
{% block header_right %}
{{ core_mobile_global.bar_nav_btn({
    title: '搜索',
    icon: 'font-f-8',
    popup: {
        title: '学区房搜索',
        popup: '.popup-search',
    }
}) }}
{% endblock header_right %}

{% block container %}
    {% set list_header %}
    <div class="filter" data-block="{
        ajax: {
            url: '{{ ('bundlebindforms/ajax')|U }}',
            data: {
                tpl: 'Common:filter',
                tag: 'xuequ'
            }
        }
    }"></div>
    {% endset %}
    {# 列表链接 js调用列表需要 #}
    {% set list_url = ('sale/xlist')|U({ajax: 1}) %}
    {{ parent() }}
{% endblock container %}



