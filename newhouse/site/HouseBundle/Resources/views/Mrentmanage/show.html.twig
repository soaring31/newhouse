{% block inner_js %}
<script type="text/javascript">
	seajs.use([ 'searchxq'], function ( searchxq) {
		$(function(){
			searchxq.search({
				xq: '#lpmc',
				url: '{{("majaxaction/xiaoqu")|U({type: 1})}}',
				autofill: ['address', 'name', 'aid','map','lng','lat']
			});
			
			searchxq.changeTitle({
				offon: '{{id}}'
			});
			
		});
	})	
</script>
{% endblock %}
