        <!--/热门楼盘-->

        <!--友情链接-->
        {% set yqlj = dqArr['yqlj']|default('') %}
        {% if yqlj|length > 0%}
        <div class="footer-list">
            <div class="title">友情链接</div>
            <ul class="link-ul">
                {% for item in yqlj|default('') %}
                <li>
                    <a href="{{item.url|default('#')}}" target="_blank">{{item.name|default('')|trans}}</a>
                </li>
                {% endfor %}
            </ul>

