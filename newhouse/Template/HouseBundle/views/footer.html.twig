    <div class="line"></div>
    <div class="container">
        <!-- 快捷入口 -->
        <div class="footer-list">
            <div class="title">快捷入口</div>
            <div class="link-ul">
                {{core_global.switch_area({
                    act_class: 'btn-main',
                    is_manage: is_manage|default(0),
                    _action: _action,
                    _suffix: _suffix,
                    tpl: '<a href="%url%" data-tag="%ename%" class="footer-a">%name%地产网</a>'
