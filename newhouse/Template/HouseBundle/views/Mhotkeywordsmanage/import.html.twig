{% extends "HouseBundle::base_show.html.twig" %}
{% block js %}

{% endblock %}
{% block body %}
{{ core_manage.form(form|default(''),'form', 'house.news') }}

<script>
  seajs.use('', function(){
    setTimeout(function(){
      var uploader = $('[data-upload]').data('uploader');
      uploader.on('uploadSuccess', function(file, res){
        if(res.status){
          $('.img-url').val(file.name);
          $.jqModal.tip(res.info, 'success');
        }
      });  
    },200);
      
  })
</script>
{% endblock %}