                                    "appName":'zgzf',
                                    "platformType":'2'
                                },
                                success:function (xhr) {
                                    var strHtml = '';
                                    var className = '';
                                    if(xhr && xhr.code ==200 && xhr.data && xhr.data.list && xhr.data.list.length >0 ){
                                        var dataList = xhr.data.list;
                                        for(var i=0;i<dataList.length;i++){
                                            if(i%2 == 0){
                                                className = 'odd';
                                            }else{
                                                className = '';
                                            }
                                    strHtml += '<li class=" '+className+'">\
                                            <p class="area-p ellipsis-p">[  '+dataList[i].cityarea+ ' '+dataList[i].cityarea2+'  ]  '+dataList[i].borough_name+'</p>\
                                            <p class="title-p ellipsis-p">\
                                                <a href="http://{{cityad}}.house.zhuge.com/'+dataList[i].borough_id+'/'+dataList[i].id+'.html">'+dataList[i].house_title+'</a>\
                                            </p>\
                                            <p class="price-p">'+dataList[i].min_price+'万</p>\
                                        </li>';
                                        }
                                        $('.price-ul').html(strHtml);
                                        $('.rightli').html('优选好房');
                                        listWd();
                                    }else{
                                        $('.rightli').remove();
                                        listWd();
                                    }
                                }
                            });
                        }
                    }
                });
                // 底部24小时降价房源
                $.ajax({
                    url:'{{ externalLinks }}'+"/API/House/search",
                    type:"post",
                    data:{
                        "city":"{{cityad}}",
                        'pageStart': '1',
                        'pageLimit': '6',
                        'houseType': '1',
                        'filter': {"tag":{"content":"1102"}},
                        "token":"bY0ffBrxJ8grLM9pDlV6Hb7xfeQHIwBV9JzRImukuu--StK6RyAcjbg6yEwMO_-3yLkpjrLq1FBvlCPfwsWoVg%3D%3D",
                        "is_cms_top": 1,
                        "appName":'zgzf',
                        "platformType":'2'
                    },
                    success:function (xhr) {
                        if(xhr && xhr.code ==200 && xhr.data && xhr.data.list && xhr.data.list.length >=6 ){
                            var dataList = xhr.data.list;
                            var strHtml= '';
                            var cName = '';
                            var agent = '';
                            var downPrice ;
                            for(var i=0 ; i < dataList.length;i++){
                                agent = '';
                                if((i+1)%3 == 0){
                                    cName = '';
                                }else{
                                    cName = "mr-20";
                                }
                                if(dataList[i].agency && dataList[i].agency.length>0){
                                    for(var j=0;j<dataList[i].agency.length;j++){
                                        agent += '<div class="list_box">\
                                                    <img src="'+dataList[i].agency[j].small_logo_url+'">\
                                                </div>';
                                    }

                                }
                                if((Number(dataList[i].history_price) - Number(dataList[i].min_price)).toString().indexOf('.')>0){
                                    downPrice = Number(Number(dataList[i].history_price) - Number(dataList[i].min_price)).toFixed(2);
                                }else{
                                    downPrice = Number(Number(dataList[i].history_price) - Number(dataList[i].min_price));
                                }
                              strHtml +=   '<li class="'+cName+'">\
                                    <a class="homepage_jjfy_'+(i+1)+'" href="http://{{cityad}}.house.zhuge.com/'+dataList[i].borough_id+'/'+dataList[i].id+'.html?pageEntrance=xinfang" target="_blank">\
                                        <div class="img_div" style="background: url('+dataList[i].house_thumb+');background-size: cover;background-position: center center;">\
                                        <p class="price-down">'+dataList[i].company_name+' 降'+downPrice+'万</p>\
                                        </div>\
                                        <div class="title_box">\
                                            <h5>['+dataList[i].cityarea+dataList[i].cityarea2+']'+dataList[i].house_title+'</h5>\
                                            <p>'+dataList[i].min_price+'万</p>\
                                        </div>\
                                        <div class="describe">'+dataList[i].house_totalarea+'平 | '+dataList[i].house_room+'室'+dataList[i].house_hall+'厅 | '+dataList[i].borough_name+'</div>\
                                        <div class="platform_box">\
                                            <p>委托中介：</p>\
                                            <div class="platform_list">'+agent+'</div>\
                                        </div>\
                                    </a>\
                                </li>';
                            }
                            $('.house-ul').html(strHtml);
                            $('.house-title-price').html('24小时降价房源');
                        }else{
                            // 没有24小时降价房源时，读取默认列表数据
                            $.ajax({
                                url:'{{ externalLinks }}'+"/API/House/search",
                                type:"post",
                                data:{
                                    "city":"{{cityad}}",
                                    'pageStart': '1',
                                    'pageLimit': '6',
                                    'houseType': '1',
                                    "token":"bY0ffBrxJ8grLM9pDlV6Hb7xfeQHIwBV9JzRImukuu--StK6RyAcjbg6yEwMO_-3yLkpjrLq1FBvlCPfwsWoVg%3D%3D",
                                    "appName":'zgzf',
                                    "platformType":'2'
                                },
                                success:function (xhr) {
                                    if(xhr && xhr.code ==200 && xhr.data && xhr.data.list && xhr.data.list.length >0 ){
                                    var dataList = xhr.data.list;
                                    var strHtml= '';
                                    var cName = '';
                                    var agent = '';
                                    for(var i=0 ; i < dataList.length;i++){
                                        agent = '';
                                        if((i+1)%3 == 0){
                                            cName = '';
                                        }else{
                                            cName = "mr-20";
                                        }
                                        if(dataList[i].agency && dataList[i].agency.length>0){
                                            for(var j=0;j<dataList[i].agency.length;j++){
                                                agent += '<div class="list_box">\
                                                            <img src="'+dataList[i].agency[j].small_logo_url+'">\
                                                        </div>';
                                            }

                                        }
                                      strHtml +=   '<li class="'+cName+'">\
                                        <a class="homepage_jjfy_'+ (i+1) +'" href="http://{{cityad}}.house.zhuge.com/'+dataList[i].borough_id+'/'+dataList[i].id+'.html?pageEntrance=xinfang" target="_blank">\
                                            <div class="img_div" style="background: url('+dataList[i].house_thumb+');background-size: cover;background-position: center center;">\
                                            </div>\
                                            <div class="title_box">\
                                                <h5>['+dataList[i].cityarea+dataList[i].cityarea2+']'+dataList[i].house_title+'</h5>\
                                                <p>'+dataList[i].min_price+'万</p>\
                                            </div>\
                                            <div class="describe">'+dataList[i].house_totalarea+'平 | '+dataList[i].house_room+'室'+dataList[i].house_hall+'厅 | '+dataList[i].borough_name+'</div>\
                                            <div class="platform_box">\
