            = jqduang：幻灯片、选项卡、slider
            = placeholder：兼容低版本输入框文字提示
            = autocomplete：搜索框自动补全提示
        */
        seajs.use([
            'jqhoverS',
            'jqduang',
            'advertise',
            'lazyload',
            'cookie'
            ], function (jqhoverS, jqduang, advertise, lazyload,cookie) {
            var resetUrl = '';
            //搜索选择
            $('.search-cate').on('mouseover', 'a', function() {
                var $this = $(this);
                var pama = $this.data("pama");
                var $searchInput = $(pama.target || '.search-input');
                var url = $this.attr('href');
                resetUrl = url;
                var type = $this.attr('type');
                if(type == 'house'){
                    $('.search-input').attr('name', 'type');
                    $this.addClass("act").siblings().removeClass("act")
                    .closest('form').attr('action', url).attr('type','house');
                }else{
                    $this.addClass("act").siblings().removeClass("act")
                    .closest('form').attr('action', url).attr('type','xinfang');
                }
                $searchInput
                    .val('')
                    .attr('placeholder', pama.searchword)
                    .autocomplete('hide')
                    // 是否启用自动完成
                if (pama.autocomplete === 0) {
                    $searchInput.autocomplete('disable');
                } else {
                    $searchInput.autocomplete('enable')
                        .autocomplete('setOptions', {
                            serviceUrl: url
                        });
                };
            });
            // banner轮播判断
            if($('.slider').length <= 1 ){
                $('.prev-btn').hide();
                $('.next-btn').hide();
            }

            // 关键词搜索 二手房 租房切换系统
            $('.search-input').on('input',function(){
                var type = $('.search-form').find('form').attr('type');
                var url = '';
                if(type == 'house'){
                    var keyValue = $(".search-input").val();
                    if(keyValue.trim()){
                        url = resetUrl + 'v_'+keyValue.trim()+'_'
                        $('.search-form').find('form').attr('action',url)
                    }

                }

            });
            // 热销楼盘 品牌楼盘 24小时降价房源 切换
            $('.j_indextabSelection li').on('click',function(){
                var index = $(this).index();
                $(this).addClass('active').siblings().removeClass('active');
                $('.j_indextabItem').eq(index).addClass('activeList').siblings().removeClass('activeList');
            });
            $('.j_indextabSelection li').on('mouseover',function(){
                var index = $(this).index();
                $(this).addClass('active').siblings().removeClass('active');
                $('.j_indextabItem').eq(index).addClass('activeList').siblings().removeClass('activeList');
            });
            var tabIndex = 0;
            var timer = null;
            var indexIncrease = function(){
                var maxLength = $('.j_indextabSelection li').length;
                tabIndex++ ;
                $('.j_indextabSelection li:eq('+ tabIndex +')').trigger('click')

                if(tabIndex > maxLength){
                    tabIndex  =  -1
                }
            }

            timer = setInterval(indexIncrease,3000);

            function closeAd(){
                if($.cookie('bpadisshow') ){

                    return;

                }


                if($('.index-bp-ad').attr('src')) {
                    $('.select_bg').addClass('ad-show');

                    $('.select_city_wrap').addClass('ad-show');

                    $('body').addClass('fix');
                }

                if($('.select_city_wrap').hasClass('ad-show')){

                    clearInterval(timer);

                }

                $('.j_cls_ad').click(function(){

                    $('.select_bg').removeClass('ad-show');

                    $('.select_city_wrap').removeClass('ad-show');

                    $('body').removeClass('fix');

                    timer = setInterval(indexIncrease,3000);

                    $.cookie('bpadisshow', '1');

                })

            }

            closeAd();

            // 计算热销楼盘 品牌楼盘 24小时降价房源 盒子宽度
            function listWd(){
                var length = $('.j_indextabSelection li').length;
                $('.j_indextabSelection li').css('width',(1200-2*length)/length);
                if(length==0){
                    $('.j_tabConter').hide();
                }else{
                    $('.j_tabConter').show();
                }
                $('.j_tabConter').mouseover(function(){

                    clearInterval(timer)

                }).mouseout(function(){

                    timer = setInterval(indexIncrease,3000)

                })

            }
            listWd();
            //slider左右箭头的显示隐藏
            $("#slider-wrap").hover(function() {
                $(this).find(".prev-btn,.next-btn").css("opacity", ".8");
            }, function() {
                $(this).find(".prev-btn,.next-btn").css("opacity", "0");
            });
            //首页header浮动导航
            roll()
            function roll() {
                $(window).on("scroll",function () {
                    $(".header_").addClass("header_b").removeClass("header_w");
                    var scrollTop = $(document).scrollTop();
                    if(scrollTop >= '580') {
                        $(".header_").addClass("header_w").removeClass("header_b").removeClass("header_z");
                        $(".select_").addClass("select_city_change").removeClass("select_city");
                        $(".speed_open_list li .nava").addClass("change_a").removeClass("change");
                        $("#logo_roll").addClass("logo_roll").removeClass("logo");
                        $("#download_roll").addClass("download_roll").removeClass("download");
                        $('.login-a').addClass('color-333').removeClass('color-fff');
                    }else{
                        $(".header_").addClass("header_b").removeClass("header_w").removeClass("header_z");
                        $(".select_").addClass("select_city").removeClass("select_city_change");
                        $(".speed_open_list li .nava").addClass("change").removeClass("change_a");
                        $("#logo_roll").addClass("logo").removeClass("logo_roll");
                        $("#download_roll").addClass("download").removeClass("download_roll");
                        $('.login-a').addClass('color-fff').removeClass('color-333');
                    }
                })
            }
            ~$(function () {
                var AdSlide = function (ele) {
                    this.index = 0;
                    this.timer = null;
                    this.$ele = $(ele);
                    this.oLis = $(ele + ' li');
                    this.len =  this.oLis.length;
                    this.eleW = this.oLis.width();
                };

                AdSlide.prototype = {
                    constructor:AdSlide,
                    init : function () {
                        var that = this;
                        var firstimg = that.oLis.first().clone(); //克隆第一张图片塞到最后
                        that.$ele.find('.ad-carousel-image').append(firstimg).width( (that.len+1) * that.eleW );
                        that.carouselAuto();
                        that.mouseoverPause();
                        that.pointsMove();
                    },

                    carouselAnimate : function () {
                        var that = this;
                        that.$ele.find('.ad-carousel-image').animate({left:-( that.eleW * that.index ) },600);
                        that.$ele.find('.ad-carousel-index li:eq('+ that.index % that.len +')').addClass('active').siblings().removeClass('active');
                    },

                    carouselAuto : function () {
                        var that = this;
                        var wrapperWidth = (that.len + 1) * that.eleW;
                        that.$ele.find('.ad-carousel-image').width(wrapperWidth);
                        if(that.len >1){
                            this.timer = setInterval(function(){
                                that.index++;
                                if (that.index > that.len) {
                                    that.index = 1;
                                    that.$ele.find('.ad-carousel-image').css({ left: 0 });
                                }
                                that.carouselAnimate();
                            },3000);
                        }
                    },

                    mouseoverPause : function () {
                        var that = this;
                        $('.ad-carousel').mouseover(function(){
                            clearInterval(that.timer);
                        }).mouseout(function(){
                            that.carouselAuto();
                        });
                    },

                    pointsMove : function () {
                        var that = this;
                        if(that.len >1){
                            for (var j = 0; j < that.len; j++) {
                                that.$ele.find('.ad-carousel-index').append('<li></li>');
                            }
                        }
                        var elePoint = that.$ele.find('.ad-carousel-index li');
                        var pointW = elePoint.width();
                        var pointWrapW = that.len * (pointW + 10);
                        that.$ele.find('.ad-carousel-index').width(pointWrapW);
                        elePoint.first().addClass('active');
                        elePoint.mouseover(function(){
                            if (!$('.ad-carousel-image').is(':animated')) {
                                that.index = $(this).index();
                                that.carouselAnimate()
                            }
                        });
                    }
                };
                var newSlide1 = new AdSlide('.ads-jujiao');
                newSlide1.init();
                var newSlide2 = new AdSlide('.ads-zunxun');
                newSlide2.init();

            });
            $('.lp-scroll').mouseover(function(){
                $('body').addClass('fix');
            }).mouseout(function(){
                $('body').removeClass('fix');
            })

