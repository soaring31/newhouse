                                if(i%2 == 0){
                                    className = 'odd';
                                }else{
                                    className = '';
                                }

                                strHtml += '<li class="'+className+'">\
                                        <p class="ell liname"><a href="http://{{cityad}}.house.zhuge.com/'+xhr.data[i].boroughId+'/'+xhr.data[i].houseId+'.html">'+xhr.data[i].title+'</a></p>\
                                        <p class="ell source">'+xhr.data[i].sourceName+'</p>\
                                        <p class="price">\
                                            <span class="origin">原价:'+xhr.data[i].historyPrice+'</span>\
                                            <span class="present">现价:'+xhr.data[i].minPrice+'万</span>\
                                        </p>\
                                        <p class="down">\
                                            <span class="iconfont icon-jiangjia"></span>'+xhr.data[i].updatePrice+'万\
                                        </p>\
                                        <p class="drop">降幅：'+xhr.data[i].updatePercent+'</p>\
                                    </li>';
                            }
                            $('.price-ul').html(strHtml);
                        }else{
                            // 没有24小时降价房源时，读取默认列表数据
                            $.ajax({
                                url:'{{ externalLinks }}'+"/API/House/search",
