{# 当前分站id #}
{% set sess_area = app.session.get('area')|default(1) %}
{# 当前城市的简称 #}
{% set city = ('db.auth_group_area')|getAll({
    rulesarea:sess_area,
    pageSize: 1,
    order   : 'sort',
    orderBy : 'asc'
}) %}
{% set cityad = city.data[0].city|default('bj') %}
{% set cityid = city.data[0].id|default('1') %}
{% set City_id = ('db.auth_group_area')|getOne('id',{rulesarea:sess_area}) %}
{% set cYCity_id = 100+City_id %}



{% set majorNav = ('house.navs')|getTree({cate_pushs: 'zdh', checked: 1, pageSize: 100, order: "sort", orderBy: 'asc'}) %}
{% set default_themes = 'default_themes'|C %}
{% block header %}
    {% include default_themes ~ '::header.html.twig' %}
{% endblock header %}
{% block footer %}
    {% include default_themes ~ '::footer.html.twig' %}
{% endblock footer %}

