{% extends 'CoreBundle::base.html.twig' %}
{% import "HouseBundle:Macro:global.html.twig" as house_global %}
{% set version = '?vt=0.0.1' %}
{# JS #}
{% block js_base %}
<script type="text/javascript" src="{{ asset('static/' ~ bundlepath ~ '/js/seajs.cfg.js', absolute=true) }}{{version}}"></script>
<script type="text/javascript">
var web = "frontEnd"; //前台
seajs.use([
    'jqhoverS',
    'fixed',
    'common'
], function(jqHoverS, fixed, common) {
    $(function() {
        // console.log('执行。。。');
        // 调用统计
        common.getcounts({
            url: '{{("igetcounts/index")|U({} ,true)}}'
        });
        // 广告统计
        common.adcount({
            saveUrl : '{{("viewinter/vote")|U}}'
        });
        // 表单处理
        common.form({
            ajax: {
                // 获取表单的默认地址
                url: '{{('bundlebindforms/ajaxform')|U}}',
            }
        })
    })
});
</script>
