{% block js %}
    <script type="text/javascript" src="{{ asset('static/house/js/echarts.js') }}{{version}}"></script>
    <script type="text/javascript">
        seajs.use([], function () {
            $(function () {
                //隐私声明
                var host= window.location.host;
                var url = " http://"+ host +"/houses/statement ";
                $(".user-statement").attr("href",url);

                var type_type;
                // 放大
                var all_height=$(window).height();
                var big_height=all_height* 0.66;
                var big_width=big_height/0.67;
                $('.shadow_1').css({'width':big_width});
                $('.big_banner').css({'height':big_height,'width':big_width});
                $('.big_block').css({'height':big_height+10,'width':big_width});
                $('.small_').click(function () {
                    var i=0,j=0;
                    var type=$(this).attr('data-type');
                    type_type=type;
                    $('.small_a').each(function () {
                        var type_=$(this).attr('data-type');
                        if(type==type_){
                            if(i==0) {
                                $(this).addClass('check');
                            }
                            i=1;
                            $(this).show();
                        }else{
                            $(this).removeClass('check');
                            $(this).hide();
                        }
                        if($(this).css('display')!='none'){
                            j++;
                        }
                    });
                    var src=$('.small_a.check').data('src');
                    img(src);
                    $('.banner_pbig').width(107*j);
                    $('.shadow_b').show();
                    var btn_width=$('.banner_cast.fix').width();
                    $('.b_fixwidth').css({'width':btn_width-75});
                });


                // 下一张
                $('.click_next').click(function () {
                    var $this=$(this).siblings('.banner_p').children('.banner_a.check').next('.banner_a');
                    var display=$this.css('display');
                    var src_=$this.data('src');
                    if(src_ && display=='block') {
                        $this.addClass('check').siblings().removeClass('check');
                        img(src_)
                    }
                });
                //上一张
                $('.click_prev').click(function () {
                    var $this=$(this).prev('.banner_p').children('.banner_a.check').prev('.banner_a');
                    var display=$this.css('display');
                    var src_1=$this.data('src');
                    if(src_1 && display=='block') {
                        $this.addClass('check').siblings().removeClass('check');
                        img(src_1)
                    }
                });

                function img(src){
                    $('.big_banner').css({
                        'background': 'url(' + src + ') center center no-repeat',
                        'background-size': 'cover'
                    });
                }

                //类似轮播图
                //    点击切换上下页
                var n=0,i;
                $('.b_prev').click(function () {
                    var width=0;
                    var $this=$(this).next('.relative_b');
                    var width_=$this.width();
                    $($this.children('.banner_p').children('a')).each(function () {
                        if($(this).css('display')=='block'){
                            width+=105;
                        }
                    });
                    i=Math.floor(width/width_);
                    if(n == 0 || i==0){
                        return false;
                    }
                    n--;
                    if(n<i) {
                        $this.children('.banner_p').css({'left':(n*width_)});
                    }

                });
                $('.b_next').click(function () {
                    var width=0;
                    var $this=$(this).prev('.relative_b');
                    var width_=$this.width();
                    $($this.children('.banner_p').children('a')).each(function () {
                        if($(this).css('display')=='block'){
                            width+=105;
                        }
                    });
                    i=Math.floor(width/width_);
                    if(n==i || i==0){
                        return false;
                    }
                    n++;
                    if(n<=i) {
                        $this.children('.banner_p').css({'left': -(n*width_)});
                    }

                });

                $('.banner_a').hover(function () {
                    var type=$(this).data('type');
                    var src=$(this).data('src');
                    $(this).parents().find('.banner_cast').siblings('.banner_block').
                    children('.banner_img').css({'background':'url('+src+') center center no-repeat','background-size':'cover'});
                    $(this).parents().find('.banner_cast').siblings('.banner_block').
                    children('.banner_img').attr('data-type',type);
                    $(this).addClass('check').siblings().removeClass('check');
                });

                $('.fix_close').click(function () {
                    $('.shadow_b').hide();
                })

            });

            window.onload = function () {
                function timestampToTime(timestamp, type) {
                    var date = new Date(timestamp * 1000);//时间戳为10位需*1000，时间戳为13位的话不需乘1000
                    Y = date.getFullYear();
                    M = (date.getMonth() + 1 < 10 ? '0' + (date.getMonth() + 1) : date.getMonth() + 1);
                    D = date.getDate() + ' ';
                    h = date.getHours() + ':';
                    m = date.getMinutes() + ':';
                    s = date.getSeconds();
                    switch (type) {
                        case 1:
                            return Y + "/" + M + "/" + D;
                            break;
                        case 2:
                            return Y + "-" + M + "-" + D;
                            break;
                        case 3:
                            return M;
                            break;
                        case 4:
                            return Y + M + D + h + m + s;
                            break;
                    }
                }

                function arrSory(array) {
                    var i = 1, j, temp, key, len = array.length;
                    for (; i < len; i++) {
                        temp = j = i;
                        key = array[j];
                        while (--j > -1) {
                            if (array[j] > key) {
                                array[j + 1] = array[j];
                            } else {
                                break;
                            }
                        }
                        array[j + 1] = key;
                    }
                    return array;
                }

                function arrQc(array) {
                    var n = {}, r = [], len = array.length, val, type;
                    for (var i = 0; i < array.length; i++) {
                        val = array[i];
                        type = typeof val;
                        if (!n[val]) {
                            n[val] = [type];
                            r.push(val);
                        } else if (n[val].indexOf(type) < 0) {
                            n[val].push(type);
                            r.push(val);
                        }
                    }
                    return r;
                }
                //   房价走势
                function echart(ary, Time, cityName, lpName) {
                    var myChart = echarts.init(document.getElementById('fjzs'));
                    option = {
                        title: {
                            text: ''
                        },
                        tooltip: {
                            trigger: 'axis',
                            axisPointer: {
                                type: 'cross'
                            }
                        },
                        color: ['#2E7DFB', '#ff8400','#ff5300'],
                        legend: {
                            data: [cityName, lpName,'{{ ('db.area')|getRow({id: detail.region,checked:1}).name|default('') }}新房'],
                            bottom: 0,
                        },
                        grid: {
                            left: '3%',
                            right: '4%',
                            bottom: '10%',
                            containLabel: true
                        },
                        toolbox: {
                            feature: {
                                saveAsImage: {}
                            }
                        },
                        xAxis: {
                            type: 'category',
                            boundaryGap: false,
                            data: Time
                        },
                        yAxis: {
                            type: 'value'
                        },
                        series: ary
                    };

                    myChart.setOption(option);
                }

                $.ajax({
                    "url": '{{ apiUrl }}'+"/api/v1/house/price?lid={{ detail.id }}",
                    "type": "get",
                    "data": {},
                    "success": function (xhr) {
                        // 城市数据源
