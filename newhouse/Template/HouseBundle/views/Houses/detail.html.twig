    <script type="text/javascript">
        seajs.use(['jqduang', 'lazyload', 'jqmodal', 'share'], function (jqduang, lazyload, jqmodal, share) {
            // 幻灯片
            $('#flash').find('.prev').on('click', function () {
                $('#scroll').jqDuang(($('#flash').jqDuang('index') - 1 + 8) % 8 <= 2 ? 0 : 1);
            });
            $('#flash').find('.next').on('click', function () {
                $('#scroll').jqDuang(($('#flash').jqDuang('index') + 1) % 8 >= 5 ? 1 : 0);
            });

            // 计算活动剩余时间
            function DateDiff(sDate1, sDate2) {  //sDate1和sDate2是yyyy-MM-dd格式
                var aDate, oDate1, oDate2, iDays;
                aDate = sDate1.split("-");
                oDate1 = new Date(aDate[1] + '-' + aDate[2] + '-' + aDate[0]);  //转换为yyyy-MM-dd格式
                aDate = sDate2.split("-");
                oDate2 = new Date(aDate[1] + '-' + aDate[2] + '-' + aDate[0]);
                iDays = parseInt(Math.abs(oDate1 - oDate2) / 1000 / 60 / 60 / 24); //把相差的毫秒数转换为天数
                return iDays;  //返回相差天数
            }
            {% set now=("now")|date('Y-m-d') %}
            {% set zxhd = ('house.promotion')|getAll({pageSize:3,checked:1,aid:detail.id,enddate:'gt|'~now}) %}
            var len = '{{ zxhd.data|length }}',time='';
            for(var i=0;i<len;i++){
                if(i<=len) {
                    if (i == 0) {
                        time = '{{ zxhd.data[0]|default.enddate|default|date('Y-m-d') }}';
                    } else if(i==1){
                        time = '{{ zxhd.data[1]|default.enddate|default|date('Y-m-d') }}';
                    }else{
                        time = '{{ zxhd.data[2]|default.enddate|default|date('Y-m-d') }}';
                    }
                    $('.day' + i + '').text(DateDiff(time, '{{ ("now"|date('Y-m-d')) }}'));
                }
            }
            // 交房时间
            $('.jiaofang,.room_time').mouseover(function () {
                $('.room_time').show();
            });

            $('.jiaofang,.room_time').mouseout(function () {
                $('.room_time').hide();
            });


