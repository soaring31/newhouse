
{% extends 'HouseBundle:Houses:lp_detail_layout.html.twig' %}
{% import "HouseBundle:Macro:listMacro.html.twig" as listMacro %}
{#说明
* block_lpdt:　最新动态
* block_lphdp : 幻灯片
* block_xiangce: 楼盘图库
* block_huxing: 户型
* block_tuangou: 团购优惠
* detail_topic_arc_aid: 看房团id
* detail_topic: 看房团
* detail_prices: 历史价格
* block_loudong: 楼栋数据
* block_zhiyeguwen: 置业顾问数据
#}
{% block tkd %}
{% set seoConts = ('db.mconfig_seo')|getAll({status:1,page_type:2,is_delete:0}) %}
{% if seoConts.data[0]|default %}
    {{- house_global.tkd({
       type:'2',
       data:seoConts.data[0],
       city:cur_area_data,
       lpName:detail.name
    }) -}}
{% else %}
{{parent()}}
{% endif %}
{% endblock tkd %}
{% block top_search %}{% endblock top_search %}

{% block container_houses %}
    {#交房时间#}
    {% set jf_date = ('house.house_make')|getAll({pageSize:5,aid:detail.id|default}) %}
    {{core_global.linkcss('static/house/css/common.css?time=201804271145')}}
    {% set block_lphdp = ('house.inter_album')|getAll({cate_pushs:'lphdp', pageSize: 10, checked: 1, findType: 1,aid:detail.id|default('')}) %}

    {% set all=
        house_global.countxc({
        aid:detail.id|default(''),
        tpl:'<b rel="nofollow">%name%(%number%)</b>'}) %}
    {% set all_o = all|split('</b>') %}

    {% set all1=
        house_global.countxc({
        aid:detail.id|default(''),
        tpl:'%name%,'})
    %}
    {% set all_ol=all1|split(',') %}
    <div class="content">
        <input type="hidden" value="{{detail.id|default('')}}" id="j_houseId"/>
        {#基础信息#}
        <div name="navItem" class="loupanInfo navItem">
            <div class="loupanInfo-con">
                <div class="loupanInfo-con-info">
                    <ul>
                        <li class="loupanInfo-info-price">
                            <span>
                                <b>参考价格：</b>
                                <i>
                                     {% if detail.dj|default('') %}
                                         {{ listMacro.if_dj({value: detail.dj|default(''), tpl: '<em class="arial">%value%</em>%danwei%'})}}
                                     {% elseif detail.tdj|default('') %}
                                         <em class="arial">{{detail.tdj|default('')}}</em>万元/套起
                                     {% else %}
                                         <em class="arial">待定</em>
                                     {% endif %}
                                    {#{{ detail.dj=='' or detail.dj=='0' or detail.dj=='0.0'?'待定':detail.dj }}</i>&nbsp{{ detail.dj=='' or detail.dj==0 or detail.dj=='0.0'?"":'万' in detail.dj ? '万/套': '元/平' }}#}
                                </i>
                            </span>
                            {#<div class="loupanInfo-tongzhi">
                                <i class="iconfont icon-kehuduan"></i>
                                <span>价格说明</span>
                            </div>#}
                            <div class="loupanInfo-tongzhi dyxx-alert" data-yid="1">
                                <i class="iconfont icon-kehuduan" style="vertical-align: inherit; position: relative; left: 5px;"></i>
                                <span>变价通知我</span>
                            </div>
                            <div class="loupanInfo-tongzhi">
                                <i class="font-lp-24 c-f00"
                                   style=" vertical-align: inherit; position: relative; left: 5px;"></i>
                                <span><a href="/tools" target="_blank" style="color:rgba(153,153,153,1) ">贷款计算器</a></span>
                            </div>
                        </li>
                        <li class="loupanInfo-info-youhui">
                            <span><b>优惠折扣：</b>{{ '暂无' }}<i></i></span>
                            <div class="loupanInfo-tongzhi dyxx-alert" data-yid="2">
                                <i class="iconfont icon-kehuduan" style="vertical-align: inherit; position: relative; left: 5px;"></i>
                                <span>优惠通知我</span>
                            </div>
                        </li>
                        <li class="loupanInfo-info-phone">
                            {#<span class="iconfont icon-dianhua1"></span>#}
                            <div>
                                <div class="detail-phone-box j_phoneBox">
                                    <em class="detail-phone"></em>
                                    {% if detail.pc_tel %}
                                        <span>
                                            {{ detail.pc_tel|replace(',','')|slice(0,3)|default('') }}
                                            {{ detail.pc_tel|replace(',','')|slice(3,3)|default('') }}
                                            {{ detail.pc_tel|replace(',','')|slice(6)|default('更新中···') }}
                                        </span>
                                    {% elseif detail.tel %}
                                        <span>
                                            {{ detail.tel|replace(',','')|slice(0,3)|default('') }}
                                            {{ detail.tel|replace(',','')|slice(3,3)|default('') }}
                                            {{ detail.tel|replace(',','')|slice(6)|default('更新中···') }}
                                        </span>
                                    {% else %}
                                        <span class="hotline"></span>
                                    {% endif %}
                                </div>
                                <p>敬请致电售楼处咨询楼盘最新政策、价格详情</p>
                            </div>
                        </li>
                        <li class="loupanInfo-info-tag">
                            {{house_global.lptag({value: detail, tpl: '<span>%value%</span>', con: 'cate_type,zxcd,tslp'})}}
                            {#{% if detail.cate_type %}
                                <span>{{ ('house.category')|getOne('name', {id: detail.cate_type|default('')}) }}</span>
                            {% endif %}

                            {% if detail.zxcd %}
                                <span>{{ ('house.category')|getOne('name', {id: detail.zxcd|default('')}) }}</span>
                            {% endif %}

                            {% if detail.tslp %}
                                {% set lpts = detail.tslp|split(',') %}
                                {% for item in lpts %}
                                    {% if ('house.category')|getOne('name', {id: item|default('')}) %}
                                        <span>
                                            {{ ('house.category')|getOne('name', {id: item|default('')}) }}
                                        </span>
                                    {% endif %}
                                {% endfor %}
                            {% endif %}#}
                        </li>
                        <script>
                            seajs.use([], function() {
                                $(function () {
                                    $('.loupanInfo-info-tag>span').each(function () {
                                        if(!$(this).text()){
                                            $(this).remove();
                                        }
                                    })
                                })
                            })
                        </script>
                        {% set room_data= ('house.door')|getAll({aid:detail.id}) %}
                        {% if room_data.data[0].shi|default or room_data.data[1].shi|default or room_data.data[2].shi|default %}
                        <li class="loupanInfo-info loupanInfo-info-suojin">
                            <span><b>主力户型：</b></span>
                            <p>
                                <span class="huxing-suojin">
                                    {% for key,item in room_data.data %}
                                        {% if key < 3 %}
                                            {% if item.shi and item.mj %}
                                                <em>、</em>{{ ('house.category')|getOne('name', {id: item.shi})~"（ 建面"~item.mj~"平米 ）" }}
                                            {% endif %}
                                        {% endif %}
                                    {% endfor %}
                                </span>
                            </p>
                            <a class="more newhouse_detail_zlhx_ckqb" href="{{ ('houses/dplanintro')|U({id: detail.id|default('')}) }}">[ 查看全部 ]</a>
                        </li>
                        {% endif %}
                        <li class="loupanInfo-info">
                            <span><b>项目地址：</b></span>
                            <p> {% set catyarea_name = ('db.area')|getRow({id: detail.region|default,checked:1}) %}
                            {% set catyarea2_name = ('house.cate_circle')|getRow({id: detail.cate_circle|default,checked:1}) %}
                            {% if catyarea_name.name|default('') or catyarea2_name.name|default('') %}
                                [ {{catyarea_name.name|default('')}} {% if catyarea_name.name|default('') and catyarea2_name.name|default('')  %} - {% endif %} {{catyarea2_name.name|default('')}} ]

                            {% endif %}{{ detail.address }} </p>
                            <a class="more newhouse_detail_xmdz_ckdt" href="{{ ('houses/map')|U({id: detail.id|default('')}) }}">&nbsp[ 查看全部 ]</a>
                        </li>
                        <li class="loupanInfo-info loupanInfo-time">
                            <span><b>开盘时间：</b></span>
                            {% set kpdate = detail.kpdate.date|split(' ') %}
                            {% if kpdate[0] == "1970-01-01" %}
                                <p>待定</p>
                            {% else %}
                                <p>{{ kpdate[0] }}</p>
                            {% endif %}
                            <div class="loupanInfo-tongzhi jiaofang">
                                <i class="iconfont icon-shijian" style="width:13px;height:13px;vertical-align: inherit; position: relative; left: 5px;"></i>
                                <span>交房时间</span>
                            </div>
                            <div class="loupanInfo-tongzhi dyxx-alert" data-yid="3">
                                <i class="iconfont icon-kehuduan" style="vertical-align: inherit; position: relative; left: 5px;"></i>
                                <span>开盘通知我</span>
                            </div>
                            <div class="room_time">
                                <div style="position:relative;border:1px solid #eeeeee;">
                                    {#<div class="top_sanjiao"><span class="mydiv"></span></div>#}
                                    <div class="room_body">
                                        <p class="top_room">{{ detail.name }}交房时间</p>
                                        <p class="room_list co">
                                            <i>交房时间</i>
                                            <i>交房详情</i>
                                        </p>
                                        {% if jf_date.data|default %}
                                            {% for item in jf_date.data %}
                                                {% set len = jf_date.data|length %}
                                                <p class="room_list {% if loop.index%2!=0 %}cc{% endif %}">
                                                    <i>{{ item.jfrq|default('暂无') }}</i>
                                                    {% autoescape %}
                                                    <i style="text-overflow:ellipsis;overflow:hidden;white-space:nowrap;">{{ item.details|raw|default('暂无') }}</i>
                                                    {% endautoescape %}
                                                </p>
                                            {% endfor %}
                                        {% else %}
                                            <p class="room_list">
                                                <i>暂无</i>
                                                <i style="text-overflow:ellipsis;overflow:hidden;white-space:nowrap;">暂无</i>
                                            </p>
                                        {% endif %}
                                        <a class="room_list a" href="{{ ('houses/ddetail')|U({id: detail.id|default('')}) }}">更多楼盘详情>></a>
                                    </div>
                                </div>
                            </div>
                        </li>
                        <li class="loupanInfo-info-more">
                            <a href="{{ "/xinfang/ddetail/"~detail.id~".html" }}" class="newhouse_detail_gdxxxx">更多详细信息&nbsp&gt;&gt;</a>
                            <div>
                                {#{{ core_global.collect_btn({
                                    aid: detail.id|default(''),
                                    models: detail.models|default('houses'),
                                    class: 'form-bdrs sc-box btn btn-sm fr'
                                }) }}#}
                                {#
                                <div class="bdsharebuttonbox share-box btn mr5 fr"
                                     share-text="{{ detail.name|default('') }}"
                                     share-desc="{{ detail.name|default('') }}"
                                     share-pic="{{ core_global.img_url({src: detail.thumb|default(''), type: 0, absolute: 1 }) }}"
                                     share-url="{{ ('houses/detail')|U({id:detail.id|default('')}, true) }}">
                                    <a data-cmd="more" class="bds_more share-a newhouse_detail_fx">
                                        <i class="font-icons-32"></i>分享
                                    </a>
                                </div>
                                #}
                            </div>
                        </li>
                    </ul>
                </div>
                {#置业顾问#}
                <div class="loupanInfo-con-people real_block" style="{% if zygw_data and zygw_data.data|length>0 %}display:block;{% else %}display:none;{% endif %}">
                    <h5>置业顾问：</h5>
                    {% for item in zygw_data.data %}
                        <div data-tel="{{ item.tel }}" data-id="{{ item.id }}">
                            <div class="pic">
                                <img src="{{ asset(item.thumb) }}" alt="" style="width:100%;height:100%;border-radius:50%;" class="real_img">
                            </div>
                            <span class="name real_name">{{ item.name }}</span>
                        </div>
                    {% endfor %}
                </div>
            </div>

            <div class="fl" style="width:600px;" data-tag="lphdp">
                <div class="banner_block">
                {% for item in 1..9 %}
                    {% set img=(('house.inter_album')|getRow({checked:1,cate_album:item,aid:detail.id|default('')})).thumb|default %}
                    {% if img %}
                         {% if 'http' in img %}
                             <div class="banner_img small_" style="background:url({{ img }}) center center no-repeat;background-size:cover;" data-type="{{ item }}"></div>
                         {% else %}
                             {% set img_url = "/"~img %}
                             <div class="banner_img small_" style="background:url('{{ img_url }}') center center no-repeat;background-size:cover;" data-type="{{ item }}"></div>
                         {% endif %}
                    {% endif %}
                {% endfor %}
                </div>
                <div class="banner_cast">
                    <span class="b_prev">&lsaquo;</span>
                    <div class="relative_b">
                        <p class="banner_p" style="width:calc((10 * 101px) + (9 * 5px))">
                            {% set i=0 %}
                            {% for key,item in 1..9 %}
                                {% set img=(('house.inter_album')|getRow({checked:1,cate_album:item,aid:detail.id|default('')})) %}
                                {% if img %}
                                    {% if 'http' in img.thumb %}
                                    <a class="banner_a {% if loop.index==1 %}check{% endif %}" href="{{ ('houses/dphotos')|U({id: detail.id|default('')}) }}" style="background:url('{{ img.thumb }}') center center no-repeat;background-size:cover;" data-type="{{ item }}" data-src="{{ img.thumb }}">
                                        <i>
                                            {{ all_ol[i] }}
                                        </i>
                                    </a>
                                    {% else %}
                                        {% set img_url = "/"~ img.thumb %}
                                        <a class="banner_a {% if loop.index==1 %}check{% endif %}" href="{{ ('houses/dphotos')|U({id: detail.id|default('')}) }}" style="background:url('{{ img_url }}') center center no-repeat;background-size:cover;" data-type="{{ item }}" data-src="{{ img_url }}">
                                            <i>
                                                {{ all_ol[i] }}
                                            </i>
                                        </a>
                                    {% endif %}
                                    {% set i=i+1 %}
                                {% endif %}
                            {% endfor %}
                        </p>
                    </div>
                    <span class="b_next">&rsaquo;</span>
                </div>
                <!-- /flash 幻灯片 -->
                <div class="blank0"></div>
            </div>
        </div>

        {# 活动 #}
        {% if block_promotion_first|default('') %}
            <div name="navItem" class="navItem">
                <div class="item_title">
                    <b>最新活动</b>
                </div>
                <div class="loupan-huodong mt20">
                    <ul>
                        {% for key,item in block_promotion.data %}
                            <li class="loupan-huodong-list">
                                <div class="loupan-huodong-cont">
                                    <div class="loupan-huodong-pic"
                                         style="background:url('{{ '/'~item.thumb }}') center center no-repeat;background-size:cover;"></div>
                                    <div class="loupan-huodong-xinxi">
                                        <h5>{{ item.name }}</h5>
                                        <div>{{ item.subname }}</div>
                                        <span>{{ item.description }}</span>
                                        <p>报名倒计时：<span class="day{{ key }}"></span>天</p>
                                    </div>
                                </div>
                                <div class="loupan-huodong-baoming">
                                    {#<a href="#hd" class="btn-jqModal-ex">立即报名</a>#}
                                    <a class="enroll newhouse_detail_zxhd_ljbm" data-id="{{ item.id }}">立即报名</a>
                                    <p>参与人数 <span>{{ item.hdnum }}</span>人</p>
                                </div>
                            </li>
                        {% endfor %}
                    </ul>
                </div>
                {#{% if zxsf %}
                    <div class="hot-bt">
                        <a href="{{ zxsf[0].url }}" name="{{ zxsf[0].name }}">
                            {{- core_global.img({
                                src: zxsf[0].thumb,
                                type: 2,
                            }) -}}
                        </a>
                    </div>
                {% endif %}#}
                <!-- AdSame ShowCode: PC / 新房详情页 / 旗帜1 Begin -->
                <script type="text/javascript" src="http://tf.zhuge.com/s?z=zgf&c={{ cYCity_id~"13103104" }}" charset="gbk" ></script>
                <!-- AdSame ShowCode: PC / 新房详情页 / 旗帜1 End -->
            </div>
            {#  活动弹窗 #}
            {#{{ house_global.modal_layout1({#}
            {#id: 'hd',#}
            {#title: '活动报名',#}
            {#ajax: "ajax: {#}
            {#data : {#}
            {#formflag: 'view_promotion_order',#}
            {#formcls: 'common-form-lg common-form-control',#}
            {#promotion_id: " ~ block_promotion.data[0].id|default(0) ~  "#}
            {#}#}
            {#}"#}
            {#}) }}#}
        {% endif %}

        {#动态资讯#}
        {% if dtzx_data and dtzx_data.data|length>0 %}
            <div name="navItem" class="navItem">
                <div class="item_title">
                    <b>动态资讯</b>
                    {#<a class="find-more " href="javascript:;">查看更多&nbsp<span>>></span></a>#}
                </div>
                <div class="dtzx">
                    <ul>
                        {% for key,item in dtzx_data.data %}
                            <li {% if key < 2 and dtzx_data.data|length>2 %}class="border-bottom"{% endif %} {% if key == 0 or key == 2 %}style="margin-right: 30px" {% endif %}>
                                <span>新闻</span>
                                <div>
                                    <a class="newhouse_detail_dtzx_{{loop.index}}" href="{{ item.url }}">
                                        <h3>{{ item.name }}</h3>
                                        <p style="height:34px;">
                                            {{ item.abstract }}
                                        </p>
                                    </a>
                                </div>
                            </li>
                        {% endfor %}
                    </ul>
                </div>
                {#{% if dtsf %}
                    <div class="hot-bt">
                        <a href="{{ dtsf[0].url }}" name="{{ dtsf[0].name }}">
                            {{- core_global.img({
                                src: dtsf[0].thumb,
                                type: 2,
                            }) -}}
                        </a>
                    </div>
                {% endif %}#}

                <!-- AdSame ShowCode: PC / 新房详情页 / 旗帜2 Begin -->
                <script type="text/javascript" src="http://tf.zhuge.com/s?z=zgf&c={{ cYCity_id~'13103105' }}" charset="gbk" ></script>
                <!-- AdSame ShowCode: PC / 新房详情页 / 旗帜2 End -->

                {#<div class="view_details">#}
                    {#<a class="pc_new_zhoubian newhouse_detail_dtzx_ckqb" href="{{ "/news/list/317" }}">查看更多</a>#}
                {#</div>#}

            </div>
        {% endif %}

        {#楼盘动态#}
        {% if lpdt_data and lpdt_data.data|length>0 %}
            <div name="navItem" class="navItem">
                <div class="item_title">
                    <b>{{ detail.name }}动态</b>
                </div>
                <div class="section">
                    <div class="dynamic-box">
                        <div class="dynamic-title">
                            <div class="dynamic-logo"><span class="iconfont icon-dongtai"></span></div>
                            <div class="loupan_name">{{ detail.name }}-楼盘动态</div>
                            <div class="new-img"><span class="iconfont icon-new"></span></div>
                        </div>
                        <i class="line"></i>
                        <ul class="dynamic_list">
                            {% for k,item in lpdt_data.data %}
                                {% if k < 2 %}
                                    <li>
                                        <p class="time">{{ item.create_time|date('m/d') }}</p>
                                        <div class="dynamic">
                                            <p class="dynamic_p">{{ item.name }}</p>
                                            <div class="dynamic_dsc">{{ item.content|raw }}</div>
                                        </div>
                                    </li>
                                {% endif %}
                            {% endfor %}
                        </ul>
                    </div>
                </div>
                <div class="time_btn"><a class="pc_new_dongtai newhouse_detail_lpdt_ckqblpdt" href="{{ "/xinfang/dnews/"~detail.id~".html" }}">查看全部楼盘动态</a>
                </div>
                <div class="view_details">
                    <a class="pc_new_zhoubian newhouse_detail_lpdt_ckxq" href="{{ "/xinfang/dnews/"~detail.id~".html" }}">查看更多&nbsp<span>>></span></a>
                </div>
                {#{% if hxsf %}
                    <div class="hot-bt">
                        <a href="{{ hxsf[0].url }}" name="{{ hxsf[0].name }}">
                            {{- core_global.img({
                                src: hxsf[0].thumb,
                                type: 2,
                            }) -}}
                        </a>
                    </div>
                {% endif %}#}
                <!-- AdSame ShowCode: PC / 新房详情页 / 旗帜3 Begin -->
                <script type="text/javascript" src="http://tf.zhuge.com/s?z=zgf&c={{ cYCity_id~'13103106' }}" charset="gbk" ></script>
                <!-- AdSame ShowCode: PC / 新房详情页 / 旗帜3 End -->
            </div>
        {% endif %}

        {#楼盘户型#}
        {% if lphx_data and lphx_data.data %}
            <div name="navItem" class="navItem">
                <div class="item_title">
                    <b>楼盘户型</b>
                </div>
                <ul class="huxing">
                    {% for k,item in lphx_data.data %}
                        {% if k < 4 %}
                            <li>
                                <a class="pc_new_hxListItem" href="{{ ('door/detail')|U({id: item.id|default('')}) }}">
                                    <div class="hx_img">
                                        <span class="img"
                                              style="background:url({{ asset(item.thumb) }}) center center;"></span>
                                    </div>
                                    <div class="hx_text">
                                        <h1 class="hx_status">
                                            {{ (('house.category')|getOne('name', {id:item.shi})~ ('house.category')|getOne('name', {id:item.ting}))|default('暂无') }}
                                            {% set status = ('house.category')|getOne('name', {id:item.cate_status}) %}
                                            <i class="hx_s {% if status=='在售' %}zs{% elseif status=='待售' %}ds{% else %}sq{% endif %}">{{ status }}</i>
                                        </h1>
                                        <div class="hx_area">
                                            建面{{ item.mj }}平米
                                        </div>
                                        <div class="hx_price">
                                            总价 :
                                            <span>
                                    <b>
                                       {{ (item.mj*item.dj)>0?(item.mj*item.dj):"待定" }}
                                    </b>
                                </span>
                                        </div>
                                    </div>
                                </a>
                            </li>
                        {% endif %}
                    {% endfor %}
                </ul>
                <div class="consult">
                    <div>
                        <a class="pc_new_hxListItem" href="{{ ('houses/dplanintro')|U({id:detail.id|default('')}) }}">全部({{- core_global.count({models: 'door', sql_opt: {aid: detail.id|default} }) -}})</a>
                        {{- house_global.countshi({
                            aid: detail.id|default(''),
                            tpl:'<a rel="nofollow" href="'~('houses/dplanintro')|U({id:detail.id|default('')})~'#shi=%shi%">%name%(%number%)</a>'}) -}}
                    </div>
                    <div class="consult_input">
                        <a class="pc_new_hxList newhouse_detail_lphx_ckqbhxfx"
                           href="{{ ('houses/dplanintro')|U({id:detail.id|default('')}) }}">查看全部&nbsp{{ detail.name }}
                            户型分析&nbsp<span>>></span></a>
                    </div>
                </div>
