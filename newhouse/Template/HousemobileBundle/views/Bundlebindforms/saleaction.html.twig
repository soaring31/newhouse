{% extends 'HousemobileBundle:Bundlebindforms:bundlebase.html.twig' %}
{% set aid = app.request.get("aid")|default() %}
{% set popup = app.request.get("popup")|default('') %}
{% set title = app.request.get("title")|default('') %}
{% set submit = app.request.get("submit")|default('') %}
{% set models = app.request.get("models", 'sale') %}
{% set cate_pushs = app.request.get("cate_pushs")|default('') %}
{% block result %}
{{- core_mobile_forms.popup_form({
    popup: popup,
    Form: Form,
    formName: formName,
    title: title,
    aid: aid,
    submit: submit,
    models: models,
    cate_pushs: cate_pushs
}) -}}
{% endblock result %}