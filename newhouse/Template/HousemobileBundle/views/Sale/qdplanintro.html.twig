{# {% extends app.request.get('ajax') ? 'HousemobileBundle:Clist:door_list.html.twig' : 'HousemobileBundle:Sale:qdetail.html.twig' %} #}
{% set tpl = app.request.get('tpl', app.request.get('ajax', '')) %}
{% extends 'HousemobileBundle:' ~ tpl|default('Sale:qdetail') ~ '.html.twig' %}
{# 说明 
* detail：小区详情数据
#}

{% block title %}
{{detail.name|default('')}}-户型图
{% endblock title %}

{% block key_des %}
<meta name="keywords" content="{{detail.name|default('')}}户型,{{detail.name|default('')}}户型图,{{detail.name|default('')}}户型大全" />
<meta name="description" content="{{detail.name|default('')}}户型图,{{cur_area_data|default('')}}{{detail.name|default('')}}小区网为您提供最新、最全的{{detail.name|default('')}}户型图，包含一居室户型、二居室户型、三居室户型等所有大小户型图，查找{{detail.name|default('')}}户型图就上{{cur_area_data|default('')}}{{core_global.mconfig({name: 'hostname', ename: 'mwebset'})}}。 "/>
{% endblock key_des %}

{% block header_title %}
    {{core_mobile_global.title({title: '小区户型图'})}}
{% endblock header_title %}

{% set nav_houses_act = 4 %}

{% block container %}
    {{core_mobile_global.list_scroll({
        content_cls: 'xq-fixed-content',
        url: ('sale/qdplanintro')|U({
	        tpl: 'Clist:door_list',
	        id: detail.id|default(''),
	    }),
        footer: 0
    })}}
    <div data-block="{
        bParams: true,
        ajax:{
            url: '{{ ('houses/dplanintro')|U }}',
            data: {
                id: '{{ detail.id|default('') }}',
                tpl: 'Clist:planintro'
            }
        }
    }"></div>
{% endblock container %}