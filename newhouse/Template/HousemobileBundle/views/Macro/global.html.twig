{# mobile seo tkd #}
{#
/**
 * @description seo title keywords description
 * @param {type} 必须 - 1为列表页，2为楼盘详情页，3为房价走势
 * @param {data} 必须 - 查表所得seo模板
 * @param {city}  必须- 城市名
 * @param {pageCount}  非必须- 列表页筛选房源总数
 * @param lpName  非必须- 详情页楼盘名称
 * @example
 * ```twig
 * {{- house_global.tkd({type: type,city: city,data:data}) -}}
 * ```
 */
 #}
{%- macro tkd(o) -%}
    {% set opt = {
       type:'',
       data:'',
       city:'',
       pageCount:'',
       lpName:'',
       phoneNum:''
    }|merge(o)%}
    {# 检索筛选数据源 #}
    {# % set cate_data = _self.cate_data({
        tag: 'houses',
        switch: {
            cate_circle: 1,
            cate_line: 1,
            hxs: 1
        }
    }).__toString|json_decode % #}
    {# % set selected = cate_data.selected|default('') % #}
    {% set cityName = opt.city|default('')|slice(0,-1) %}
    {# % set regionData = selected.cate_circle|default('') ? selected.cate_circle.name|default('') : selected.region.name|default('') % #}
    {# % set metroData = selected.cate_metro|default('') ? selected.cate_metro.name|default('') : selected.cate_line.name|default('') % #}
    {% set title = '' %}
    {% set keywords = '' %}
    {% set description = '' %}
    {% if opt.type == '1' %}
        {# % set seoCont = opt.data|default('') % #}
        {% set title = '【#城市#新房总价_新房户型__新楼盘_新开楼盘信息】-#城市#诸葛找房网'|replace({
            '#城市#':cityName,
        }) %}
        {% set keywords = '#城市#新楼盘，#城市#新房价格 , #城市#新房网'|replace({
            '#城市#':cityName
        }) %}
        {% set description = '#城市#诸葛找房新房网频道-汇聚全网#城市#新房总价 , 新房户型 , 新房房源#实时楼盘数量#个新楼盘, 提供即时新开楼盘信息,新房房源信息,让您全面了解#城市#新房总价 , 新房户型 , 新开楼盘信息等,诸葛找房房地产界的搜索引擎,买房购房查找房源首选#城市#诸葛找房网'|replace({
            '#城市#':cityName,
            '#实时楼盘数量#':opt.pageCount|default('')
        }) %}
    {% endif %}

    {% if opt.type == '2' %}
        {# % set seoCont = opt.data|default('') % #}
        {% set title = '【#楼盘名#_楼盘详情】-#城市#诸葛找房网'|replace({
            '#城市#':cityName,
            '#楼盘名#':opt.lpName|default(''),
        }) %}
        {% set keywords = '#楼盘名#楼盘信息 , #楼盘名#楼盘详情'|replace({
            '#楼盘名#':opt.lpName|default(''),
        }) %}
        {% set description = '北京诸葛找房网提供#楼盘名#售楼处电话#楼盘电话#楼盘地址、楼盘价格，楼盘动态，楼盘点评，全网比价等信息'|replace({
            '#城市#':cityName,
            '#楼盘名#':opt.lpName|default(''),
            '#楼盘电话#':opt.phoneNum|default('')
        }) %}
    {% endif %}

    {% spaceless %}
        <title>{{title}}</title>
        <meta name="keywords" content="{{keywords}}">
        <meta name="description" content="{{description}}">
    {% endspaceless %}
{%- endmacro -%}
{#
/**
* @method content(o)
* @description 内容简介部分
* @param {String} o.title=内容简介 - 标题
* @param {String} o.content - 内容介绍数据
* @param {String} o.conclass - 样式
* @param {Boolean} o.btn - 是否显示更多按钮
* @param {String} o.default - 没有内容的时候显示
* @example 调用
* ```twig
* {{- housemobile_global.content({title: '内容简介', content: houses_detail.content|default('')})-}}
* ```
*/
#}
